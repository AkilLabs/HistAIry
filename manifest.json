{
  "manifest_version": 3,
  "name": "Simple History Viewer",
  "version": "1.0",
  "description": "Shows recent Chrome history",
  "permissions": ["history"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png"
    }
  },
  "host_permissions": ["<all_urls>"]
}
