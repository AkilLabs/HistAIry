{
  "manifest_version": 3,
  "name": "History Assistant - AI Chat & Analytics",
  "version": "2.0",
  "description": "AI-powered browsing history viewer with intelligent chat assistant and analytics",
  "permissions": ["history", "tabs", "sidePanel"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png"
    }
  },
  "side_panel": {
    "default_path": "dist/sidebar.html"
  },
  "host_permissions": ["<all_urls>"]
}
