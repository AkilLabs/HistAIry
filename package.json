{
  "name": "history-extension-scripts",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd server && node index.js",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build:extension",
    "install:all": "cd frontend && npm install && cd ../server && npm install",
    "start:server": "cd server && node index.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.0"
  }
}
